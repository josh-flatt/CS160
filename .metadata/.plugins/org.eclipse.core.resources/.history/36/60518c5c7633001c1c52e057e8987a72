package cs160lab7code;
import java.util.*;
public class Source
{
public static void main(String args[])
{
    Scanner s = new Scanner(System.in);
    String N = s.nextLine();
    int n = Integer.parseInt(N);
    System.out.print("It's "+n+"o'clock!");
    for (int i=0;i<n;i++)
        System.out.print(" Cuckoo!");
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
}
